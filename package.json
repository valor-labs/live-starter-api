  },
  "pre-commit": [
    "tslint"
  ],
  "dependencies": {
    "@types/core-js": "0.9.41",
    "@types/express": "4.0.35",
    "@types/lodash": "4.14.64",
    "@types/mime": "0.0.29",
    "@types/mongoose": "4.7.13",
    "@types/node": "7.0.23",
    "@types/promise": "7.1.30",
    "async": "2.4.1",
    "body-parser": "1.17.2",
    "cors": "2.8.3",
    "express": "4.15.3",
    "express-redis-cache": "0.5.1",
    "lodash": "4.17.4",
    "mime": "1.3.6",
    "mongoose": "4.10.4",
    "morgan": "1.8.2",
    "nconf": "0.8.4",
    "rxjs": "5.4.0",
    "ts-node": "3.0.4",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@angular/compiler": "4.1.3",
    "@angular/core": "4.1.3",
    "codelyzer": "3.0.1",
