{
  "name": "live-starter-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tsc && ./node_modules/.bin/ts-node app.ts",
    "tsc": "./node_modules/.bin/tsc --project tsconfig.json",
    "watch": "./node_modules/.bin/tsc -w ./routes/**/*.ts ./routes/*.ts",
    "build:dev": "./node_modules/.bin/tsc",
    "flow.tslint": "./node_modules/.bin/tslint ./routes/**/*.ts -c tslint.json",
    "flow.lint": "npm run flow.tslint"
  },
  "pre-commit": [
    "flow.lint"
  ],
  "dependencies": {
    "@angular/compiler": "4.0.0-rc.2",
    "@angular/core": "4.0.0-rc.2",
    "@types/core-js": "0.9.36",
    "@types/express": "4.0.35",
    "@types/lodash": "4.14.54",
    "@types/mime": "0.0.29",
    "@types/mongoose": "4.7.8",
    "@types/node": "7.0.7",
    "@types/promise": "7.1.30",
    "async": "2.1.5",
    "body-parser": "1.17.1",
    "cors": "2.8.1",
    "eslint-config-valorsoft": "0.1.0",
    "express": "4.15.2",
    "express-redis-cache": "0.5.0",
    "lodash": "4.17.4",
    "mime": "1.3.4",
    "mongoose": "4.8.6",
    "morgan": "1.8.1",
    "nconf": "0.8.4",
    "rxjs": "5.2.0",
    "ts-node": "2.1.0",
		"typescript": "2.2.1"
  },
  "devDependencies": {
    "codelyzer": "3.0.0-beta.3",
    "gitignore-to-glob": "0.3.0",
    "pre-commit": "1.2.2",
    "tslint": "4.5.1",
    "tslint-config-valorsoft": "1.2.0",
    "typescript": "2.2.1",
    "zone.js": "0.7.7"
  },
  "engines": {
    "node": "7.7.2"
  }
}
